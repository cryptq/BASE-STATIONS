<Item XName="Equations" Type="Equation">
<Selected><T>Bool</T><D>0</D></Selected>
<Text>% Parameters Range Check&#x000D;&#x000A;if (FCarrier_In&lt;=0)&#x000D;&#x000A;&#x0009;error(&apos;FCarrier_In should be greater than 0&apos;);&#x000D;&#x000A;end&#x000D;&#x000A;if (FCarrier_Out&lt;=0)&#x000D;&#x000A;&#x0009;error(&apos;FCarrier_Out should be greater than 0&apos;);&#x000D;&#x000A;end&#x000D;&#x000A;if (Foffset_Interferer&lt;=-FCarrier_In)&#x000D;&#x000A;&#x0009;error(&apos;Foffset_Interferer should be greater than -FCarrier_In&apos;);&#x000D;&#x000A;end&#x000D;&#x000A;if (P_WantedSignal&lt;0)&#x000D;&#x000A;&#x0009;error(&apos;P_WantedSignal should be greater than or equal to 0&apos;);&#x000D;&#x000A;end&#x000D;&#x000A;if (P_Interferer&lt;0)&#x000D;&#x000A;&#x0009;error(&apos;P_Interferer should be greater than or equal to 0&apos;);&#x000D;&#x000A;end&#x000D;&#x000A;if (NF&lt;0)&#x000D;&#x000A;&#x0009;error(&apos;NF should be in range [0, inf)&apos;);&#x000D;&#x000A;end&#x000D;&#x000A;if (UE1_MappingType&lt;0 || UE1_MappingType&gt;2)&#x000D;&#x000A;&#x0009;error(&apos;UE1_MappingType should be integer in range [0, 2]&apos;);&#x000D;&#x000A;end&#x000D;&#x000A;if (UE1_Config==0 &amp;&amp; (UE1_Payload&lt;-1 || UE1_Payload&gt;28 || mod(UE1_Payload,1)~=0))&#x000D;&#x000A;&#x0009;error(&apos;When UE1_Config is set to &quot;MCS index&quot;, the integer value in UE1_Payload should be in range [-1, 28] (-1 means no block is allocated)&apos;);&#x000D;&#x000A;end&#x000D;&#x000A;if (UE1_Config==1 &amp;&amp; (UE1_Payload&lt;0 || mod(UE1_Payload,1)~=0))&#x000D;&#x000A;&#x0009;error(&apos;When UE1_Config is set to &quot;Transport block size&quot;, the integer value in UE1_Payload should be in range [0, inf) (0 means no block is allocated)&apos;);&#x000D;&#x000A;end&#x000D;&#x000A;if (UE1_Config==2 &amp;&amp; (UE1_Payload&lt;0 || UE1_Payload&gt;10))&#x000D;&#x000A;&#x0009;error(&apos;When UE1_Config is set to &quot;Code rate&quot;, the float value in UE1_Payload should be in range [0, 10] (0 means no block is allocated)&apos;);&#x000D;&#x000A;end&#x000D;&#x000A;if (EnableTxFilter==1 &amp;&amp; (FIR_Taps&lt;1 || FIR_Taps&gt;1000))&#x000D;&#x000A;&#x0009;error(&apos;When EnableTxFilter is enabled, FIR_Taps should be integer in range [1, 1000]&apos;);&#x000D;&#x000A;end&#x000D;&#x000A;if (NumFrames&lt;1)&#x000D;&#x000A;&#x0009;error(&apos;NumFrames should be integer in range [1, inf)&apos;);&#x000D;&#x000A;end&#x000D;&#x000A;&#x000D;&#x000A;% calculate the sampling rate&#x000D;&#x000A;SamplingTable = [1.92 3.84 7.68 15.36 23.04 30.72];&#x000D;&#x000A;SamplingFreq = 1e6 * SamplingTable(Bandwidth+1);&#x000D;&#x000A;SamplingRate = SamplingFreq * (2^OversamplingOption);&#x000D;&#x000A;SampsPerSF = SamplingRate * 1e-3;&#x000D;&#x000A;&#x000D;&#x000A;BW_Table = [1.4,3,5,10,15,20]*1e6;&#x000D;&#x000A;RB_Table = [6,15,25,50,75,100];&#x000D;&#x000A;RS_EPRE = -10*log10(RB_Table(Bandwidth+1)*12);&#x000D;&#x000A;FCarrier_Interferer = FCarrier_In + Foffset_Interferer;&#x000D;&#x000A;&#x000D;&#x000A;P_WantedSignal_dBm = 10*log10(P_WantedSignal) + 30;&#x000D;&#x000A;RS_EPRE_WantedSig = P_WantedSignal_dBm - 10*log10(RB_Table(Bandwidth+1)*12);&#x000D;&#x000A;if Bandwidth &gt;= 2&#x000D;&#x000A;&#x0009;Bandwidth_Interferer = 2; %5MHz&#x000D;&#x000A;&#x0009;Interferer_Interp = Bandwidth - 1;&#x000D;&#x000A;else&#x000D;&#x000A;&#x0009;Bandwidth_Interferer = Bandwidth;&#x000D;&#x000A;&#x0009;Interferer_Interp = 1;&#x000D;&#x000A;end&#x000D;&#x000A;&#x000D;&#x000A;UseInterp = 2*(Bandwidth &lt;= 2);&#x000D;&#x000A;&#x000D;&#x000A;UE1_RB_Alloc_Interferer = [0,RB_Table(Bandwidth_Interferer+1)];&#x000D;&#x000A;P_Interferer_dBm = 10*log10(P_Interferer) + 30;&#x000D;&#x000A;RS_EPRE_Interferer = P_Interferer_dBm - 10*log10(RB_Table(Bandwidth_Interferer+1)*12);;&#x000D;&#x000A;&#x000D;&#x000A;TransmissionBW = RB_Table(Bandwidth+1) * 180e3;&#x000D;&#x000A;ChannelBW = BW_Table(Bandwidth+1);&#x000D;&#x000A;&#x000D;&#x000A;NumTotalRB = RB_Table(Bandwidth+1);&#x000D;&#x000A;%%% Users can move following parameters in the &quot;Parameters&quot; tab if necessary&#x000D;&#x000A;TDD_Config = 1;&#x000D;&#x000A;SpecialSF_Config = 4;&#x000D;&#x000A;CyclicPrefix = 0; % Normal CP&#x000D;&#x000A;RB_MappingType = 0; % Localized&#x000D;&#x000A;RB_Gap = 0;&#x000D;&#x000A;UEs_TransMode = [0,0,0,0,0,0];&#x000D;&#x000A;UEs_SpecificRS = [0,0,0,0,0,0];&#x000D;&#x000A;UE1_Category = 4;&#x000D;&#x000A;UE1_NumHARQ = 8 - FrameMode;&#x000D;&#x000A;UE1_MaxHARQTrans = 4;&#x000D;&#x000A;UE1_RV_Sequence = [0,1,2,3];&#x000D;&#x000A;RB_AllocType = 0;&#x000D;&#x000A;PDCCH_SymsPerSF_Define = [2,2,2,2,2,2,2,2,2,2]; % PDCCH symbol per subframe when UseFRC is NO.&#x000D;&#x000A;&#x000D;&#x000A;if (EnableTxFilter == 1)&#x000D;&#x000A;&#x0009;SpectrumShapingType = 1;&#x000D;&#x000A;&#x0009;FIR_withInterp = 0;&#x000D;&#x000A;&#x0009;FIR_FilterType = 1;&#x000D;&#x000A;&#x0009;RRC_Alpha = 0.22;&#x000D;&#x000A;&#x0009;WindowType = 0;&#x000D;&#x000A;&#x0009;CyclicInterval = 6;&#x000D;&#x000A;&#x0009;CI_StartPos = -3;&#x000D;&#x000A;else&#x000D;&#x000A;&#x0009;SpectrumShapingType = 0;&#x000D;&#x000A;&#x0009;WindowType = 0;&#x000D;&#x000A;&#x0009;CyclicInterval = 0;&#x000D;&#x000A;&#x0009;CI_StartPos = 0;&#x000D;&#x000A;&#x0009;FIR_withInterp = 0;&#x000D;&#x000A;&#x0009;FIR_FilterType = 1;&#x000D;&#x000A;&#x0009;RRC_Alpha = 0.22;&#x000D;&#x000A;end &#x000D;&#x000A;&#x000D;&#x000A;if UseFRC == 1&#x000D;&#x000A;&#x0009;UE1_MappingType = [0];&#x000D;&#x000A;&#x0009;DemapperMaxLevel = 100.0;&#x000D;&#x000A;&#x0009;UE1_Config = 1;&#x000D;&#x000A;&#x000D;&#x000A;&#x0009;[UE1_RB_Alloc, UE1_Payload, PDCCH_SymsPerSF] = LTE_getDLFRC_A_3_2(FrameMode, Bandwidth);&#x000D;&#x000A;else&#x000D;&#x000A;&#x0009;if (UE1_Config==0)&#x000D;&#x000A;&#x0009;&#x0009;if UE1_Payload &lt; 10&#x000D;&#x000A;&#x0009;&#x0009;&#x0009;UE1_MappingType = 0;&#x000D;&#x000A;&#x0009;&#x0009;elseif UE1_Payload &lt; 17&#x000D;&#x000A;&#x0009;&#x0009;&#x0009;UE1_MappingType = 1;&#x000D;&#x000A;&#x0009;&#x0009;else&#x000D;&#x000A;&#x0009;&#x0009;&#x0009;UE1_MappingType = 2;&#x000D;&#x000A;&#x0009;&#x0009;end&#x000D;&#x000A;&#x0009;end&#x000D;&#x000A;&#x000D;&#x000A;&#x0009;if UE1_MappingType==0&#x000D;&#x000A;&#x0009;&#x0009;DemapperMaxLevel = 100.0;&#x000D;&#x000A;&#x0009;else&#x000D;&#x000A;&#x0009;&#x0009;DemapperMaxLevel = 1.0;&#x000D;&#x000A;&#x0009;end&#x000D;&#x000A;&#x000D;&#x000A;&#x0009;if FullRB_Alloc==1&#x000D;&#x000A;&#x0009;&#x0009;UE1_RB_Alloc = [0, NumTotalRB];&#x000D;&#x000A;&#x0009;else&#x000D;&#x000A;&#x0009;&#x0009;UE1_RB_Alloc = [StartRB, NumRBs];&#x000D;&#x000A;&#x0009;&#x0009;if (StartRB&lt;0 || StartRB&gt;NumTotalRB-1)&#x000D;&#x000A;&#x0009;&#x0009;&#x0009;error(&apos;StartRB is out of the allowable RB range.&apos;);&#x000D;&#x000A;&#x0009;&#x0009;elseif (NumRBs&lt;1 || NumRBs&gt;NumTotalRB)&#x000D;&#x000A;&#x0009;&#x0009;&#x0009;error(&apos;NumRBs is out of the allowable RB range.&apos;);&#x000D;&#x000A;&#x0009;&#x0009;elseif (StartRB+NumRBs-1&gt;NumTotalRB-1)&#x000D;&#x000A;&#x0009;&#x0009;&#x0009;error(&apos;RB allocation is out of the allowable RB range.&apos;);&#x000D;&#x000A;&#x0009;&#x0009;end&#x000D;&#x000A;&#x0009;end&#x000D;&#x000A;&#x0009;PDCCH_SymsPerSF = PDCCH_SymsPerSF_Define;&#x000D;&#x000A;end&#x000D;&#x000A;&#x000D;&#x000A;% adjust BlockSize so the IPC overhead is negligible and still preserve reasonable latency&#x000D;&#x000A;if SampsPerSF &gt; 10000&#x000D;&#x000A;&#x0009;BlockSize = floor(SampsPerSF / 10 + 0.5);&#x000D;&#x000A;else&#x000D;&#x000A;&#x0009;BlockSize = SampsPerSF;&#x000D;&#x000A;end</Text>
